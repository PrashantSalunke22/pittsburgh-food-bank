<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{> molecules-page-header }}
		<div class="main-body l">
			<div class="l-main">
				<section class="section">
					{{> molecules-tabs }}
					<ul class="post-list event-list">
						{{# eventList }}
						<li>{{> molecules-block-event }}</li>
						{{/ eventList }}
					</ul>
				</section>
				{{> organisms-well-subscribe }}
			</div><!--end .l-main-->
			<div class="l-sidebar">
				{{> organisms-sidebar-text-list }}
				{{> organisms-sidebar-text }}
			</div><!--end .l-sidebar-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>