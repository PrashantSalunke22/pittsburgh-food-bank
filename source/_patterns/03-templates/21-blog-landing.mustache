<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		<div class="lc">
			{{> molecules-page-header }}
		</div>
		<div class="l-3col">
			<div class="l-main">
				<ul class="post-list">
					{{# postList }}
					<li>{{> molecules-block-post }}</li>
					{{/ postList }}
					{{> molecules-pagination }}
					{{> organisms-well-subscribe }}
				</ul>
			</div><!--end .l-main-->
			<div class="l-sidebar">
				{{> molecules-section-nav }}
			</div><!--end .l-sidebar-->
			<div class="l-sidebar2">
				{{> organisms-sidebar-text }}
				{{> organisms-sidebar-event-list }}
				
			</div><!--end .l-sidebar2-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>