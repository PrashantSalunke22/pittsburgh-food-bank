<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{> molecules-page-header }}
		<div class="main-body l">
			<div class="l-main">
				<h2>Awards</h2>
				<div class="g g-2up">
					{{# awardList }}
					<div class="gi">
						{{> molecules-block-thumb-title-excerpt }}
					</div>
					{{/ awardList }}
				</div>
				<hr />
				<h2>Press</h2>
				<ul class="post-list">
					{{# postList }}
					<li>
						{{> molecules-block-post }}
					</li>
					{{/ postList }}
				</ul>
				{{> organisms-well-subscribe }}
			</div><!--end .l-main-->
			<div class="l-sidebar">
				
			</div><!--end .l-sidebar-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>