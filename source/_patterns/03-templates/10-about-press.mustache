<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		<div class="lc">
			{{> molecules-page-header }}
		</div>
		<div class="l-3col">
			<div class="l-main">
				<h2>Awards</h2>
				<div class="g g-2up">
					{{# awardList }}
					<div class="gi">
						{{> molecules-block-thumb-title-excerpt }}
					</div>
					{{/ awardList }}
				</div>
				<hr />
				<h2>Press</h2>
				<ul class="post-list">
					{{# postList }}
					<li>
						{{> molecules-block-post }}
					</li>
					{{/ postList }}
				</ul>
				{{> organisms-newsletter-well }}
			</div><!--end .l-main-->
			<div class="l-sidebar">
				{{> molecules-section-nav }}
			</div><!--end .l-sidebar-->
			<div class="l-sidebar2">
				
			</div><!--end .l-sidebar2-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>