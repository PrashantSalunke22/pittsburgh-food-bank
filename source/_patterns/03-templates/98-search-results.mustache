<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{> molecules-page-header }}
		<div class="main-body l">
			<div class="l-main">
				<ul class="post-list">
					{{# postList }}
					<li>{{> molecules-block-post }}</li>
					{{/ postList }}
					{{> molecules-pagination }}
					{{> organisms-well-search }}
				</ul>
			</div><!--end .l-main-->
			<div class="l-sidebar">
				{{> organisms-sidebar-text }}
				{{> organisms-sidebar-event-list }}
			</div><!--end .l-sidebar-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>