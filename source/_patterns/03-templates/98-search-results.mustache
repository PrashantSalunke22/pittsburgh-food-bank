<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{> molecules-page-header }}
		<div class="main-body l">
			<div class="l-main">
				<div class="lc lc-single">
					<ul class="post-list">
						{{# postList }}
						<li>{{> molecules-block-post }}</li>
						{{/ postList }}
						{{> molecules-pagination }}
					</ul>
				</div>
			</div><!--end .l-main-->
			{{> organisms-well-search }}
			<div class="l-sidebar">
				{{> organisms-sidebar-text }}
				{{> organisms-sidebar-event-list }}
			</div><!--end .l-sidebar-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>