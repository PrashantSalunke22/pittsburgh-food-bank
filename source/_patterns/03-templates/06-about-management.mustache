<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		{{> molecules-pattern-banner }}
		<div class="l-3col">
			<div class="l-main">
				{{> molecules-page-header }}
				<h2>Executive Staff</h2>
				<div class="g g-half">
					{{# execList }}
					<div class="gi">
						{{> molecules-block-thumb-portrait-title }}
					</div>
					{{/ execList }}
				</div>
				<hr />
				<h2>Board of Directors</h2>
				<div class="g g-half">
					{{# boardList }}
					<div class="gi">
						{{> molecules-block-title-excerpt }}
					</div>
					{{/ boardList }}
				</div>
				
			</div><!--end .l-main-->
			<div class="l-sidebar">
				{{> molecules-section-nav }}
			</div><!--end .l-sidebar-->
			<div class="l-sidebar2">
				{{> molecules-well }}
			</div><!--end .l-sidebar2-->
		</div><!--end .l-3col-->
	</div><!--End role=main-->
	{{> organisms-footer }}
</div>